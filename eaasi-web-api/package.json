{
	"name": "eaasi-web-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"generate-docs": "apidoc -i src/ -o apidoc",
		"check-types": "tsc",
		"clean": "rimraf dist",
		"build": "npm run generate-docs && npm run clean && babel src --out-dir dist --copy-files --source-maps --extensions \".ts,.js\"",
		"lint": "eslint src --fix",
		"server:prod": "node ./dist/app.js",
		"start": "npm run prod",
		"watch": "nodemon",
		"test:unit": "npm run build && jest --verbose --logHeapUsage",
		"watch-test:unit": "jest --watch --verbose --logHeapUsage",
		"test:unit:coverage": "npm run build && jest --verbose --logHeapUsage --coverage",
		"test:e2e:open": "export NODE_ENV=test && run-p --race local:test cy:open",
		"test:e2e": "export NODE_ENV=test && run-p --race local:test cy:run",
		"local:test": "export NODE_ENV=test && npm run clean && npm run build && npm run server:prod",
		"local:test:watch": "export NODE_ENV=test && npm run clean && npm run watch",
		"local:watch": "export NODE_ENV=development && npm run clean && npm run watch",
		"dev": "export NODE_ENV=development && npm run clean && npm run build && npm run server:prod",
		"prod": "export NODE_ENV=production && npm run clean && npm run build && npm run server:prod"
	},
	"dependencies": {
		"@babel/plugin-proposal-decorators": "^7.12.1",
		"@types/bcrypt": "^3.0.0",
		"@types/nodemailer": "^6.4.0",
		"acorn": "^6.4.2",
		"bcrypt": "^4.0.1",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv-flow": "^3.2.0",
		"express": "~4.16.1",
		"express-async-handler": "^1.1.4",
		"express-rate-limit": "^5.1.3",
		"express-validator": "^6.6.1",
		"faker": "^4.1.0",
		"http-errors": "~1.6.3",
		"jade": "^1.11.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "~1.9.1",
		"multer": "^1.4.2",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.4.14",
		"npm-run-all": "^4.1.5",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"passport-oauth": "^1.0.0",
		"passport-saml": "^1.5.0",
		"pg": "^7.18.2",
		"pg-hstore": "^2.3.2",
		"reflect-metadata": "0.1.9",
		"sequelize": "^5.22.3",
		"sequelize-typescript": "^1.1.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/node": "^7.12.1",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-numeric-separator": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-typescript": "^7.12.1",
		"@babel/runtime": "^7.12.1",
		"@types/bluebird": "^3.5.33",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.8",
		"@types/express": "^4.17.8",
		"@types/express-rate-limit": "^5.1.0",
		"@types/http-errors": "^1.8.0",
		"@types/jest": "^25.2.3",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/memory-cache": "^0.2.1",
		"@types/morgan": "^1.9.2",
		"@types/node": "^12.19.3",
		"@types/node-fetch": "^2.5.7",
		"@types/passport": "^1.0.4",
		"@types/passport-jwt": "^3.0.3",
		"@types/passport-saml": "^1.1.2",
		"@types/validator": "^10.11.3",
		"@types/webpack-env": "^1.15.3",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"apidoc": "^0.17.7",
		"app-root-path": "^3.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-watch": "^7.0.0",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^24.8.0",
		"jest-sonar-reporter": "^2.0.0",
		"memory-cache": "^0.2.0",
		"nodemon": "^1.19.4",
		"prettier": "^1.19.1",
		"rimraf": "^2.6.3",
		"sequelize-mock": "^0.10.2",
		"superagent": "^5.3.1",
		"supertest": "^4.0.2",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"jest": {
		"testResultsProcessor": "jest-sonar-reporter"
	},
	"jestSonar": {
		"reportPath": "coverage",
		"reportFile": "coverage-report.xml"
	},
	"apidoc": {
		"title": "EaaSI Web API",
		"url": "http://localhost:8081/api/"
	}
}
