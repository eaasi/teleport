{
	"name": "eaasi-web-api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"generate-docs": "apidoc -i src/ -o apidoc",
		"check-types": "tsc",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"clean": "rimraf dist",
		"build": "npm run generate-docs && npm run clean && babel src --out-dir dist --source-maps --copy-files --extensions \".ts,.js\"",
		"server:prod": "node ./dist/bin/www.js",
		"start": "npm run prod",
		"test:unit": "npm run build && jest --verbose --logHeapUsage",
		"test:unit:coverage": "npm run build && jest --verbose --logHeapUsage --coverage",
		"test:e2e:open": "export NODE_ENV=test && run-p --race local:test cy:open",
		"test:e2e": "export NODE_ENV=test && run-p --race local:test cy:run",
		"local:test": "export NODE_ENV=test && npm run clean && npm run build && npm run server:prod",
		"local:test:watch": "export NODE_ENV=test && npm run clean && npm run build && nodemon server:prod",
		"local:watch": "export NODE_ENV=local && npm run clean && npm run build && nodemon --inspect ./dist/bin/www.js",
		"dev": "export NODE_ENV=development && npm run clean && npm run build && npm run server:prod",
		"dev:watch": "export NODE_ENV=development && npm run clean && npm run build && nodemon ./dist/bin/www.js",
		"prod": "export NODE_ENV=production && npm run clean && npm run build && npm run server:prod"
	},
	"dependencies": {
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/runtime": "^7.5.5",
		"@types/passport": "^1.0.0",
		"@types/webpack-env": "^1.14.0",
		"acorn": "^6.2.0",
		"body-parser": "^1.19.0",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.0.0",
		"dotenv-flow": "^3.1.0",
		"express": "~4.16.1",
		"express-validator": "^6.1.1",
		"faker": "^4.1.0",
		"http-errors": "~1.6.3",
		"jade": "^1.11.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "~1.9.1",
		"npm-run-all": "^4.1.5",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"passport-oauth": "^1.0.0",
		"passport-saml": "^1.1.0",
		"pg": "^7.4.1",
		"pg-hstore": "^2.3.2",
		"sequelize": "^5.15.10"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.0",
		"@babel/core": "^7.5.4",
		"@babel/node": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-typescript": "^7.3.3",
		"@types/bluebird": "^3.5.27",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.5",
		"@types/express": "^4.17.0",
		"@types/http-errors": "^1.6.2",
		"@types/jsonwebtoken": "^8.3.3",
		"@types/node": "^12.6.8",
		"@types/validator": "^10.11.1",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"cypress": "^3.4.1",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"jest": "^24.8.0",
		"jest-sonar-reporter": "^2.0.0",
		"nodemon": "^1.19.1",
		"prettier": "^1.18.2",
		"rimraf": "^2.6.3",
		"sequelize-mock": "^0.10.2",
		"superagent": "^5.1.0",
		"supertest": "^4.0.2",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"jest": {
		"testResultsProcessor": "jest-sonar-reporter"
	},
	"jestSonar": {
		"reportPath": "coverage",
		"reportFile": "coverage-report.xml"
	},
	"apidoc": {
		"title": "EaaSI Web API",
		"url": "http://localhost:8081/api/"
	}
}
