{
	"name": "eaasi-front-end",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve --port 8084",
		"build": "vue-cli-service build",
		"cy:run": "cypress run",
		"cy:run:styleguide": "cypress run --spec cy tests/e2e/specs/styleguide/**/*",
		"cy:open": "cypress open",
		"lint": "vue-cli-service lint --fix",
		"lint:css": "stylelint '**/*.vue' --fix --syntax scss",
		"styleguide": "vue-cli-service styleguidist",
		"styleguide:build": "vue-cli-service styleguidist:build",
		"test:e2e": "vue-cli-service test:e2e",
		"test:styleguide": "export CYPRESS_BASE_URL=http://localhost:6060 && run-p --race styleguide cy:run:styleguide",
		"test:styleguide:ci": "export CYPRESS_BASE_URL=http://localhost:6060 && cypress install && run-p --race styleguide cy:run:styleguide",
		"test:styleguide:open": "export CYPRESS_BASE_URL=http://localhost:6060 && run-p --race styleguide cy:open",
		"test:unit": "vue-cli-service test:unit --verbose"
	},
	"dependencies": {
		"@vue/cli-service": "^3.8.0",
		"core-js": "^2.6.5",
		"lodash": "^4.17.11",
		"npm-run-all": "^4.1.5",
		"typescript": "^3.4.3",
		"vue": "^2.6.10",
		"vue-class-component": "^7.0.2",
		"vue-property-decorator": "^8.1.0",
		"vue-router": "^3.0.3",
		"vue-styleguidist": "^3.19.5",
		"vuex": "^3.0.1",
		"vuex-pathify": "^1.2.4"
	},
	"devDependencies": {
		"@types/dom-inputevent": "^1.0.5",
		"@types/jest": "^23.1.4",
		"@types/lodash": "^4.14.135",
		"@vue/cli-plugin-babel": "^3.8.0",
		"@vue/cli-plugin-e2e-cypress": "^3.8.0",
		"@vue/cli-plugin-eslint": "^3.8.0",
		"@vue/cli-plugin-typescript": "^3.8.0",
		"@vue/cli-plugin-unit-jest": "^3.8.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"cypress": "^3.4.1",
		"cypress-vue-unit-test": "^1.11.3",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-vue": "^5.0.0",
		"faker": "^4.1.0",
		"husky": "^3.0.3",
		"jsdoc": "^3.6.3",
		"jsdoc-vuejs": "^3.0.0",
		"sass": "^1.18.0",
		"sass-loader": "^7.1.0",
		"stylelint": "^10.1.0",
		"stylelint-config-standard": "^18.3.0",
		"stylelint-order": "^3.0.0",
		"stylelint-processor-html": "^1.0.0",
		"stylelint-scss": "^3.9.2",
		"ts-jest": "^23.0.0",
		"vue-cli-plugin-styleguidist": "^3.19.5",
		"vue-template-compiler": "^2.6.10"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	}
}
